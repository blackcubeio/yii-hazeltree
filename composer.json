{
    "name": "blackcube/yii-hazeltree",
    "description": "PHP 8.3+ nested set (Hazeltree) implementation using rational numbers for Yii3 framework",
    "keywords": ["yii3", "nested-set", "tree", "hierarchical", "rational-numbers"],
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Philippe Gaultier",
            "email": "philippe@blackcube.io"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "blackcube/yii-magic-compose": "^1.0",
        "yiisoft/active-record": "^1.0",
        "yiisoft/cache": "^3.2",
        "yiisoft/db": "^2.0"
    },
    "require-dev": {
        "codeception/codeception": "^5.3",
        "codeception/module-asserts": "^3.3",
        "codeception/module-db": "^3.2",
        "vlucas/phpdotenv": "^5.6",
        "yiisoft/db-migration": "^2.0",
        "yiisoft/db-mysql": "^2.0",
        "yiisoft/cache-file": "^3.2",
        "yiisoft/di": "^1.4",
        "yiisoft/event-dispatcher": "^1.1",
        "yiisoft/factory": "^1.3",
        "yiisoft/test-support": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Blackcube\\Hazeltree\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blackcube\\Hazeltree\\Tests\\": "tests",
            "Blackcube\\Hazeltree\\Tests\\Support\\": "tests/Support"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-devel": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
